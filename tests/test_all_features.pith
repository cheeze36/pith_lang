# Test Break/Continue
print("Testing Break/Continue:")
int i = 0
while (i < 10):
    i = i + 1
    if (i == 5):
        continue
    if (i > 7):
        break
    print(i)

# Test Do-While
print("Testing Do-While:")
int j = 0
do:
    print(j)
    j = j + 1
while (j < 3)

# Test Switch
print("Testing Switch:")
int k = 2
switch(k):
    case 1:
        print("Case 1")
    case 2:
        print("Case 2")
    case 3:
        print("Case 3")
    default:
        print("Default")

# Test List Syntax
print("Testing List Syntax:")
list<int> my_list = [1, 2, 3]
print(my_list)
